/*add on 16 sep for Department menu*/
.listdata ul li {
  float: none;
  margin: 0;
  width: 100%;
  min-height:inherit;
}
.listdata ul li h2 {
  font-size: 10px;
  height: inherit;
  line-height:32px;
}
.listdata ul li h2 a{padding-left:8px; font-size:14px;}
.listdata ul li > div > .img_cover {
  clear: both;
  float: left;
  height: auto;
  margin-top: 6px;
  width: 40px;
}
/*.img_thumbnail_image {
  width: 40px;
  height:30px;
}*/
.listdata ul li:nth-child(n+3)
{
	display:none;
}
.listdata ul li:last-child
{
	display:block;
}
.listdata ul li .gimage {
  display: inline-block;
  margin-bottom: 0;
  width: 100%;
}
.listdata ul li:nth-child(1)
{
	margin-bottom:0px !important;
}
/*See more*/
.gimage > a {
  background: #005fbe;
  border: 1px solid #ccc;
  color: #fff;
  display: inline-block;
  font-size: 15px;
  font-weight: bold;
  line-height: 13px;
  margin-bottom: 11px;
  margin-top: 5px;
  padding: 8px 23px;
  text-decoration: none;
  text-shadow: 0px 1px 2px rgba(150, 150, 150, 1);
}
.gimage > a:hover
{
	background:#004aa9;
}
.listdata ul li > .gimage > a > .img_cover:after {
  content:"SEE All"
}
.listdata ul li > .gimage > a > .img_cover > img {
  display:none;
}
.listdata ul li:last-child .img_cover {
  height: inherit;
  margin-top: 0px;
  width: inherit;
}
.img_cover {
  box-shadow: none;
}
